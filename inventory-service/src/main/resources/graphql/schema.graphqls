type Product {
    id: ID,
    name: String,
    category: String,
    price: Float,
    stock: Int
}

type Query{
    getProducts:[Product]
    getProductByCategory(category:String):[Product]
}

type Mutation{
    updateStock(productId:ID, stock:Int):Product
    receiveNewShipment(productId:ID, quantity:Int):Product
}